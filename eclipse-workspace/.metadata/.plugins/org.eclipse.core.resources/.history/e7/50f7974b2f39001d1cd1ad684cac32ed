package variables;

public class variables {

	public static void main(String[] args) {
		int ogrenciSayisi = 11;  //camelCase kullanılır değişkenlerde
		System.out.println("Öğrenci sayım : "+ogrenciSayisi);
		System.out.println("Öğrenci sayım : "+ogrenciSayisi);
		System.out.println("Öğrenci sayım : "+ogrenciSayisi);
		System.out.println("Öğrenci sayım : "+ogrenciSayisi);
		System.out.println("Öğrenci sayım : "+ogrenciSayisi);
		System.out.println("Öğrenci sayım : "+ogrenciSayisi);

	}

}
